  pod 'Gecco', :git => 'https://github.com/shukob/Gecco.git'

  target 'PocketCarboTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'PocketCarboUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
